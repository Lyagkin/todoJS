{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    // "prefer-const": "error",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-console": "warn",
    "semi": ["warn", "always"]
  }
}
